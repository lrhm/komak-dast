<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dialog_game_result_loading_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/dialog_game_result_loading_container_relative_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ir.treeco.aftabe2.View.Custom.UserLevelView
            android:id="@+id/dialog_game_result_my_player"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            app:customDimension="0.16"
            app:isOnlineTop="true"
            app:textAlign="below" />

        <ir.treeco.aftabe2.View.Custom.UserLevelView
            android:id="@+id/dialog_game_result_op"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            app:customDimension="0.16"
            app:isOnlineTop="true"

            app:textAlign="below"

            />

    </RelativeLayout>

    <ImageView
        android:id="@+id/activity_main_loading_image_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />

</FrameLayout>
